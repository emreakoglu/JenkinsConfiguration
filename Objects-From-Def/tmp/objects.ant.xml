<?xml version="1.0" encoding="UTF-8"?><!--
    
    This ant script template is used to produce a "quickie" ant script that is run from
    within a DCM ant script.  It is expected to inherit all the important properties
    such as dcm.dir and work.dir, and no provision is made for testing for these.
    
    This template requires the ${dcm.jar} ant property.  Also the /generated/antscript
    element must contain the ant scripting to insert into the template.
    
--><project xmlns:props="antlib:ibm" name="quickie">
    
    <!-- Definitions related to the DCM jar file. -->
    <typedef classpath="${dcm.jar}" resource="com/ibm/antlib.xml" uri="antlib:ibm"/>
    <propertyhelper>
        <props:nested/>
    </propertyhelper>

  <sequential><checkDeviceAccess host="${host}" uid="${uid}" pwd="${pwd}" port="${port}" versionprop="firmware_version" dumpinput="${dumpinput}" dumpoutput="${dumpoutput}" capturesoma="${capturesoma}"/><objectCreateByInline domain="${domain}" host="${host}" uid="${uid}" pwd="${pwd}" port="${port}" versionprop="firmware_version" dcmdir="${dcm.dir}" schemadir="${schema.dir}" workdir="${work.dir}" dumpinput="${dumpinput}" dumpoutput="${dumpoutput}" capturesoma="${capturesoma}"><Matching xmlns:dcm="urn:datapower:configuration:manager" xmlns:dp="http://www.datapower.com/schemas/management" xmlns:env="http://www.w3.org/2003/05/soap-envelop" name="JenkinsMatch_1">
			<mAdminState>enabled</mAdminState>
			<MatchRules>
				<Type>url</Type>
				<HttpTag/>
				<HttpValue/>
				<Url>*</Url>
				<ErrorCode/>
				<XPATHExpression/>
				<Method>default</Method>
				<CustomMethod/>
			</MatchRules>
			<MatchWithPCRE>off</MatchWithPCRE>
			<CombineWithOr>off</CombineWithOr>
		</Matching></objectCreateByInline></sequential><sequential><checkDeviceAccess host="${host}" uid="${uid}" pwd="${pwd}" port="${port}" versionprop="firmware_version" dumpinput="${dumpinput}" dumpoutput="${dumpoutput}" capturesoma="${capturesoma}"/><objectCreateByInline domain="${domain}" host="${host}" uid="${uid}" pwd="${pwd}" port="${port}" versionprop="firmware_version" dcmdir="${dcm.dir}" schemadir="${schema.dir}" workdir="${work.dir}" dumpinput="${dumpinput}" dumpoutput="${dumpoutput}" capturesoma="${capturesoma}"><StylePolicyRule xmlns:dcm="urn:datapower:configuration:manager" xmlns:dp="http://www.datapower.com/schemas/management" xmlns:env="http://www.w3.org/2003/05/soap-envelope" name="JenkinsRule_1">
			<mAdminState>enabled</mAdminState>
			<Direction>request-rule</Direction>
			<InputFormat>none</InputFormat>
			<OutputFormat>none</OutputFormat>
			<NonXMLProcessing>off</NonXMLProcessing>
			<Unprocessed>off</Unprocessed>
		</StylePolicyRule></objectCreateByInline></sequential><sequential><checkDeviceAccess host="${host}" uid="${uid}" pwd="${pwd}" port="${port}" versionprop="firmware_version" dumpinput="${dumpinput}" dumpoutput="${dumpoutput}" capturesoma="${capturesoma}"/><objectModify domain="${domain}" host="${host}" uid="${uid}" pwd="${pwd}" port="${port}" versionprop="firmware_version" class="StylePolicy" name="sample_mpg" dcmdir="${dcm.dir}" schemadir="${schema.dir}" workdir="${work.dir}" dumpinput="${dumpinput}" dumpoutput="${dumpoutput}" capturesoma="${capturesoma}"><dcm:vector-insert-before xmlns:dcm="urn:datapower:configuration:manager" class="PolicyMaps">
		  <PolicyMaps>
			<Match class="Matching">JenkinsMatch_1</Match>
			<Rule class="StylePolicyRule">JenkinsRule_1</Rule>
		  </PolicyMaps>
		</dcm:vector-insert-before></objectModify></sequential>
    
</project>